{
  "version": 2,
  "buildCommand": "git lfs pull",
  "installCommand": "git lfs install",
  "headers": [
    {
      "source": "/assets/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        }
      ]
    }
  ]
}
